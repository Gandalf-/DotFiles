#!/usr/bin/env python3

# pydo
#   read in lines from stdin and apply the commands given from the command line
#   to them
#
#   example
#       pydo '%s[::-1]' < file.txt
#       ip addr | pydo '[ord(_) for _ in %s]'

# pylint: disable=eval-used

import sys

command = sys.argv[1]
data = [_.strip() for _ in sys.stdin.readlines()]

for line in data:
    line = '"' + line.replace('"', '\\"') + '"'
    print(eval(command % line))
